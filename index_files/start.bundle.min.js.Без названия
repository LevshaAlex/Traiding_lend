"use strict";var i,appLink=getAppLink(),OS=getOs(),storeButtons=document.querySelectorAll("[data-applink]");for(i=0;i<storeButtons.length;i++)storeButtons[i].setAttribute("href",appLink);document.querySelectorAll("body")[0].className+="iOS"==OS?"body--ios":"AndroidOS"==OS?"body--android":"body--desktop";