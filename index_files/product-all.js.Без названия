"use strict";$(function(){var s=$(".apps-buttons"),t=$(".platform-content"),o=getOs();t.css("display","inline-block"),"AndroidOS"==o||"iOS"==o?(t.css("display","none"),s.show(),"AndroidOS"==o?($(".apps-buttons__button_android").show(),$(".js-iphone-mockups").css("display","none"),$(".js-samsung-mockups").css("display","block")):"iOS"==o&&$(".apps-buttons__button_ios").show()):t.css("display","inline-block"),window.getParameters={},location.search.substr(1).split("&").forEach(function(s){var t=s.split("=");window.getParameters[t[0]]=t[1]}),"platform"==window.getParameters.product&&("AndroidOS"!=o&&"iOS"!=o||(t.css("display","inline-block"),s.hide())),window.getParameters.product,$("a[data-getAppLink]").attr("href",getAppLink())});