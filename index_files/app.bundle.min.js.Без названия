"use strict";$(function(){$(".scrollto-link").click(function(t){t.preventDefault(),$("html,body").animate({scrollTop:$($(this).attr("href")).offset().top},1e3)}),$(".js-button-bottom").on("click",function(t){t.preventDefault();var e=$(".js-about").offset().top;return $("html, body").animate({scrollTop:e},function(){}),!1})}),$(function(){var e=new RegistrationModel(RegistrationModel.FORM_2_P);e.addEmailInput(".register .login").addPasswordInput(".register .pass").addRulesInput(".register .rules").addCurrencyInput(".register__radio-input"),$(".register").on("submit",function(t){t.preventDefault(),e.execute()})});